<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Entities.Inventory" table="inventorys">
        <id name="inventoryId" type="java.lang.Integer">
            <column name="inventory_id" not-null="true"></column>
            <generator class="identity"></generator>
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" not-null="true"></column>
        </property>
        <property name="address" type="java.lang.String">
            <column name="address" not-null="true"></column>
        </property>
        <many-to-one name="dealer" class="Entities.Dealer" fetch="select">
            <column name="dealer_id" not-null="true"></column>
        </many-to-one>
    </class>
</hibernate-mapping>